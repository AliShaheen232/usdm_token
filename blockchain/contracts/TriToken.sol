contract TriToken is ERC20, Ownable {
    /// @custom:oz-upgrades-unsafe-allow constructor

    using SafeMath for uint256;

    mapping(address => mapping(address => uint256)) private _allowances;

    address private _token;

    IUniswapV2Pair public _uniswapV2Pair;
    IPriceOracle public _priceOracle;

    constructor() ERC20("TriToken", "TRI") Ownable(msg.sender) {
        _mint(msg.sender, 2000000000 * 10 ** decimals());
    }

    /////////////////////////////////////////////////

    function mint(address to, uint256 amount) public onlyOwner {
        _mint(to, amount);
    }

    receive() external payable {}

    function setUniswapPair(address p) external onlyOwner {
        _uniswapV2Pair = IUniswapV2Pair(p);
    }

    function setPriceOracle(address oracle) public onlyOwner {
        _priceOracle = IPriceOracle(oracle);
    }
}
